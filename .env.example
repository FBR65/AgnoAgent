# AgnoAgent Configuration

# LLM Configuration
API_KEY=ollama
BASE_URL=http://localhost:11434/v1

# Model Names (adjust according to your Ollama models)
LEKTOR_MODEL=qwen2.5:latest
OPTIMIZER_MODEL=qwen2.5:latest
SENTIMENT_MODEL=qwen2.5:latest
USER_INTERFACE_MODEL=qwen2.5:latest

# MCP Server Configuration
MCP_HOST=localhost
MCP_PORT=8000
MCP_SCHEME=http

# A2A Network Configuration
A2A_NETWORK_ID=agno-network
A2A_DISCOVERY_HOST=localhost
A2A_DISCOVERY_PORT=9000

# Logging Configuration
LOG_LEVEL=INFO

# Optional: Custom NTP Server
NTP_SERVER=de.pool.ntp.org
NTP_VERSION=3

# Optional: Browser Configuration
BROWSER_WAIT_TIME=5
BROWSER_HEADLESS=true
